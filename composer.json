{
  "name": "xutl/laravel-flash",
  "description": "This is a Laravel flash message.",
  "keywords": [
    "flash",
    "laravel",
    "message"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "laravel/framework": "5.6.* | 5.5.* | 5.4.* | 5.3.* | 5.2.* | 5.1.* | 5.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "XuTL\\Flash\\": "src"
    },
    "files": [
      "src/function.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "XuTL\\Flash\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "XuTL\\Flash\\FlashServiceProvider"
      ],
      "aliases": {
        "Flash": "XuTL\\Flash\\Flash"
      }
    }
  },
  "prefer-stable": true
}
