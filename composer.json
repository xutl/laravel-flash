{
  "name": "xutl/laravel-flash",
  "description": "This is a Laravel flash message.",
  "keywords": [ "flash", "laravel", "message" ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "XuTL\\Flash\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },

  "autoload-dev": {
    "psr-4": {
      "XuTL\\Flash\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "XuTL\\Flash\\FlashServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
